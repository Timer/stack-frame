{
  "name": "stack-frame-mapper",
  "version": "0.2.1",
  "description": "Maps a stack frame to a sourcemap.",
  "scripts": {
    "prepublishOnly": "npm run build && npm test",
    "build": "../../node_modules/.bin/babel src/ -d lib/",
    "test": "../../node_modules/.bin/jest"
  },
  "main": "lib/index.js",
  "repository": "https://github.com/Timer/stack-frame/tree/master/packages/stack-frame-mapper",
  "author": "Joe Haddad <timer150@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "settle-promise": "^1.0.0",
    "stack-frame": "^0.2.0",
    "stack-frame-utils": "^0.3.1"
  },
  "devDependencies": {
    "stack-frame-parser": "^0.2.1"
  },
  "jest": {
    "setupFiles": [
      "./src/__tests__/setupJest.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "setupJest.js"
    ]
  }
}
